{
  "name": "@vinxi/solid",
  "version": "0.1.1",
  "type": "module",
  "author": "Nikhil Saraf <nsaraf98@gmail.com>",
  "files": [
    "src",
    "*.jsx",
    "*.js",
    "*.d.ts"
  ],
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.jsx",
    "./server": "./lib/server.js",
    "./routes": "./lib/routes.js",
    "./client": "./lib/app-client-plugin.js",
    "./lib/*": "./lib/*.js",
    "./runtime/*": "./runtime/*.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./index.d.ts"
      ],
      "./client": [
        "./client.d.ts"
      ],
      "./server": [
        "./server.d.ts"
      ],
      "./routes": [
        "./routes.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@solidjs/meta": "^0.28.6",
    "solid-js": "^1.7.11",
    "vinxi": "0.3.12"
  }
}
