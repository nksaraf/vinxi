{
  "name": "@vinxi/plugin-mdx",
  "version": "3.7.1",
  "type": "module",
  "scripts": {
    "// User Scripts": "",
    "dev": "pnpm run tsc:watch",
    "types": "pnpm run tsc:once",
    "// TypeScript": "",
    "tsc:watch": "tsc --build --watch",
    "tsc:once": "tsc --build",
    "build:cjs": "esbuild --bundle --platform=node --external:esbuild --external:vite --external:@mdx-js/mdx src/index.ts > dist/index.cjs",
    "// Release": "",
    "prepublishOnly": "pnpm run build:cjs",
    "release": "npm version patch && npm publish && git push && git push --tags",
    "// Remove caches": "",
    "clean": "git clean -Xdff"
  },
  "main": "dist/index.cjs",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@mdx-js/mdx": "2 | 3"
  },
  "dependencies": {
    "@alloc/quick-lru": "^5.2.0",
    "esbuild": "0.20.2",
    "resolve": "^1.22.8",
    "unified": "^11.0.4",
    "vfile": "^6.0.1"
  },
  "devDependencies": {
    "@mdx-js/mdx": "3.0.1",
    "@types/es-module-lexer": "^0.3.0",
    "@types/mdast": "^4.0.3",
    "@types/node": "^20.11.30",
    "@types/resolve": "^1.20.6",
    "@types/unist": "^3.0.2",
    "typescript": "^5.4.3",
    "vite": "5.2.6"
  },
  "description": "Vite plugin for MDX",
  "license": "MIT",
  "repository": "https://github.com/brillout/vite-plugin-mdx",
  "keywords": [
    "vite",
    "mdx"
  ],
  "publishConfig": {
    "access": "public"
  }
}
