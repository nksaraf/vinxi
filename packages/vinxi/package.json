    "perfect-debounce": "^1.0.0",
    "radix3": "^1.1.0",
    "resolve": "^1.22.6",
    "rollup-plugin-visualizer": "^5.9.2",
    "serve-placeholder": "^2.0.1",
    "serve-static": "^1.15.0",
    "ufo": "^1.3.0",
    "unenv": "^1.7.4",
    "unimport": "^3.2.0",
    "unstorage": "^1.9.0",
    "vite": "4.5.0",
    "ws": "^8.13.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/node": "^18.17.11",
    "@types/resolve": "^1.20.6",
    "prettier": "^2.8.8"
  }
}
