    "@types/micromatch": "^4.0.2",
    "@types/serve-static": "^1.15.2",
    "@types/ws": "^8.5.5",
    "@vinxi/devtools": "workspace:^",
    "@vinxi/listhen": "^1.5.5",
    "boxen": "^7.1.1",
    "c12": "^1.4.2",
    "chokidar": "^3.5.3",
    "citty": "^0.1.4",
    "consola": "^3.2.3",
    "defu": "^6.1.2",
    "dts-buddy": "^0.2.4",
    "es-module-lexer": "^1.3.0",
    "esbuild": "^0.18.20",
    "fast-glob": "^3.3.1",
    "get-port": "^6.1.2",
    "get-port-please": "^3.1.1",
